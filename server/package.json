{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/src/index.js",
    "dev": "concurrently \"npm:dev:tsc\" \"npm:dev:run\"",
    "dev:tsc": "tsc --watch",
    "dev:run": "nodemon --watch dist --exec node dist/src/index.js",
    "seed": "npm run build && shx rm -rf dist/prisma/seedData && shx cp -r prisma/seedData dist/prisma/seedData && node --env-file=.env dist/prisma/seed.js",
    "prisma:generate": "prisma generate",
    "postprisma:generate": "shx cp -r node_modules/.prisma/client/index.d.ts ../client/src/types/prismaTypes.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@terraformer/wkt": "^2.2.1",
    "axios": "^1.13.4",
    "body-parser": "^2.2.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "prisma": "^7.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "@types/terraformer__wkt": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
